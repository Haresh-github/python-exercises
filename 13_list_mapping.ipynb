    "def count_super_vowels(s):\n",
    "    super_vowels = {'a', 'e', 'i', 'o', 'u'}\n",
    "    count = 0\n",
    "    for char in s.lower():\n",
    "        if char in super_vowels:\n",
    "            count += 1\n",
    "    return count\n",
    "\n",
    "string = \"This is an example string to count super vowels.\"\n",
    "print(count_super_vowels(string))\n"
   ]
  }
